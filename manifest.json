{
  "name": "Blue Contributions",
  "version": "0.0.1",
  "description": "Turns your GitHub contribution graph blue.",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": [
    "https://github.com/*"
  ],
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}